{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranches": [
    "develop"
  ],
  "automerge": true,
  "packageRules": [
    {
      "matchPackagePatterns": [
        ".*"
      ],
      "rangeStrategy": "bumpMinor"
    }
  ]
}
